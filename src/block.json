{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "blocks-course/latest-posts",
	"version": "0.1.0",
	"title": "Latest Posts",
	"category": "widgets",
	"icon": "admin-post",
	"description": "Display a list of most recent posts",
	"keywords": ["latest", "posts"],
	"supports": {
		"html": false
	},
	"textdomain": "latest-posts",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"attributes": {
		"numberOfPosts": {
			"type": "number",
			"default": 3		
		},
		"displayFeaturedImage": {
			"type": "boolean",
			"default": true
		}
	}
}
